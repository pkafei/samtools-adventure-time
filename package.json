{
  "name": "get-SAMtools",
  "version": "1.0.0",
  "description": "workshop content for learning SAMtools",
  "main": "index.js",
  "scripts": {
    "render": "submarine markdown guide --header='SAMtools Adventure-time' --footer='<a href=\"https://github.com/pkafei/samtools-adventure-time\">SAMtools on github</a>'",
    "deploy": "gh-pages-deploy",
    "build": "browserify index.js -o bundle.js && browserify terminal.js -o terminal-bundle.js",
    "start": "beefy index.js:bundle.js terminal.js:terminal-bundle.js"
  },
  "author": "portia burton",
  "license": "BSD",
  "watch": {
    "build": "workshop/*.md"
  },
  "gh-pages-deploy": {
    "prep": [
      "build",
      "render"
    ],
    "noprompt": "true"
  },
  "dependencies": {
    "adventure-time": "^1.0.0",
    "gh-pages-deploy": "^0.1.1",
    "submarine": "0.0.10"
  },
  "devDependencies": {
    "beefy": "^2.1.1",
    "browserify": "^6.1.0",
    "npm-watch": "0.0.0"
  }
}
